{
  "name": "blog-starter-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "gen-rss": "node ./scripts/gen-rss.js",
    "gen-sitemap": "node --experimental-modules ./scripts/gen-sitemap.mjs",
    "build": "yarn gen-rss && yarn gen-sitemap && next build && next export",
    "export": "next export",
    "start": "next start",
    "typecheck": "tsc",
    "pretty-quick": "pretty-quick",
    "test": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx,.js --fix"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "date-fns": "2.21.3",
    "gray-matter": "4.0.3",
    "next": "12",
    "next-mdx-remote": "^3.0.2",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.24.1",
    "react": "17",
    "react-dom": "17",
    "reading-time": "^1.3.0",
    "remark": "13.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "13.0.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/prismjs": "^1.16.6",
    "@types/react": "17",
    "@types/react-dom": "17",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^27.0.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "cypress": "^7.6.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "12",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "globby": "^12.0.0",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.5",
    "lint-staged": ">=10",
    "postcss": "^8.3.0",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "rss": "^1.2.2",
    "serve": "^12.0.0",
    "tailwindcss": "^2.1.2"
  },
  "license": "MIT",
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.10"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@4.9.2"
}
